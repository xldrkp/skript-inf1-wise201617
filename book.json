{
  "language": "de",
  "plugins": [
    "image-captions", "-sharing", "forkmegithub", "downloadpdf", "youtube", "fancybox"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "image-captions": {
      "caption": "Abbildung _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    },
    "forkmegithub": {
      "url": "https://github.com/xldrkp/skript-inf1-wise201617",
        "color": "red"
    },
    "downloadpdf": {
      "base": "https://github.com/xldrkp/skript-inf1-wise201617/blob/master/book.pdf",
      "label": "Download PDF",
      "multilingual": false
    }
  },
  "pdf": {
    "fontSize": 14
  },
  "styles": {
    "website": "./styles/website.css",
    "pdf": "./styles/pdf.css"
  }
}
